x=input().split()
b=int(input())
y=input().split()
c=int(input())
z=input().split()
list1=[]
for i in x:
    if((i in y) and (i in z)):
        list1.append(i)
if(len(list1)==0):
    print("NO Elements")
else:
    print(*list1)

