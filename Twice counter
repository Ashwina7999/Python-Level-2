PROGRAM 1:
from collections import Counter
for i in range(int(input())):
     a=int(input())
     b=dict(Counter(input().strip().split()))
     count=0
     for i,j in b.items():
          if(j==2):
               count+=1
     print(count)          


PROGRAM 2:
n=int(input())
for i in range(n):
    a=int(input())
    list1=[]
    for i in range(1):
        b=input().split()
    for i in b:
       c=b.count(i)
       if(c==2):
           list1.append(i)      
    d=set(list1)
    print(len(d))


